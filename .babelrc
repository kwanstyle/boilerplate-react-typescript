{
    "presets": [
        [
            "@babel/preset-env",
            {
                // Set to false if tree-shaking in Webpack is affected
                "modules": "auto"
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": {
                    "version": 3,
                    "proposals": true
                },
                "useESModules": true
            }
        ],
        "@babel/plugin-proposal-class-properties"
    ]
}
